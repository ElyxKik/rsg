{
  "name": "rsg-frontend",
  "version": "1.0.0",
  "description": "Site institutionnel public - Réserve Stratégique Générale",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
