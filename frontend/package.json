    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "security-check": "npm audit && snyk test"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "tailwindcss": "^3.3.0",
    "framer-motion": "^10.16.0",
    "gsap": "^3.12.0",
    "lottie-react": "^2.4.0",
    "react-hook-form": "^7.48.0",
    "zod": "^3.22.0",
    "@tanstack/react-query": "^5.25.0",
    "zustand": "^4.4.0",
    "next-intl": "^3.0.0",
    "mapbox-gl": "^2.15.0",
    "lucide-react": "^0.292.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0"
